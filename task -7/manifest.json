{
  "manifest_version": 3,
  "name": "Amfoss Weathercaster",
  "version": "1.0",
  "description": "A Chrome extension that displays the weather by the name of the area.",
  "permissions": [
    "tabs",
    "geolocation"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}